#include <string>
#include <vector>

using namespace std;
int fibo_arr[100001];

int fibo(int n){
    if(n == 1 || n == 2) return 1;
    if(fibo_arr[n] != 0) return (fibo_arr[n]%1234567);
    else return fibo_arr[n] = (fibo(n-1) + fibo(n-2))%1234567;
}

int solution(int n) {
    int answer = 0;
    answer = fibo(n);
    return answer;
}
